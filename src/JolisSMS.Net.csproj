<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>jkulubya.JolisSMS.Net</PackageId>
    <PackageVersion>0.0.1</PackageVersion>
    <Authors>https://twitter.com/jkulubya</Authors>
    <Description>Send SMS via the Jolis.net API</Description>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageLicenseUrl>https://github.com/jkulubya/JolisSMS.Net/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/jkulubya/JolisSMS.Net</PackageProjectUrl>
    <PackageTags>jolis.net SMS</PackageTags>
    <RepositoryUrl>https://github.com/jkulubya/JolisSMS.Net</RepositoryUrl>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Flurl" Version="2.6.0" />
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
  </ItemGroup>
</Project>